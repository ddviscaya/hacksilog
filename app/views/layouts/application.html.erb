<!DOCTYPE html>
<html>
  <head>
    <title>Hacksilog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag "style", media: "all" %>
    <%= stylesheet_link_tag "bootstrap.min", media: "all" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'main', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'jquery.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'jquery-migrate.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap.bundle.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'easing.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'mobile-nav', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'wow.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'waypoints.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'counterup.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'owl.carousel.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'isotope.pkgd.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'lightbox.min', 'data-turbolinks-track': 'reload' %>

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Montserrat:300,400,500,700" rel="stylesheet">

    <%= stylesheet_link_tag "font-awesome.min", media: "all" %>
    <%= stylesheet_link_tag "animate.min", media: "all" %>
    <%= stylesheet_link_tag "ionicons.min", media: "all" %>
    <%= stylesheet_link_tag "owl.carousel.min", media: "all" %>
    <%= stylesheet_link_tag "lightbox.min", media: "all" %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
