<main id="main">
  <!--==========================
    Services Section
  ============================-->
  <div class="container">
    <div class="row">
      <div class="col-4" style="padding: 0 !important;">
        <img src="https://image.freepik.com/free-vector/funny-corgi-dog-paws-up-white-wall_42750-483.jpg" style="width: 130px;">
      </div>
      <div class="col-8" style="padding: 0 !important;">
        lol
      </div>
    </div>
  </div>

  

  <section id="services" class="section-bg" style="padding-top: 30 px;">
    <div class="container" style="padding-bottom: 40px;">
      <% if notice -%>
        <p class="alert alert-success"><%= notice %></p>
      <% end -%>
      <% if alert -%>
        <p class="alert alert-danger"><%= alert %></p>
      <% end -%>
      <section id="new-goal-form">
        <div class="container-fluid">

          <div class="section-header text-center">
            <h5>Set your new goal, Beautiful!</h5>
          </div>

          <div class="row wow fadeInUp">
              <div class="form align-items-center">
                <form action="/goals" method="post" role="form" class="contactForm">
                  <div class="form-row align-items-center">
                    <div class="form-group col-lg-6">
                      <label>Category</label>
                      <select required="true" name="category_id" class="form-control">
                        <option disabled selected value> -- Select a Category -- </option>
                          <%@categories.each do |c|%>
                            <option value="<%= c.id %>"><%= c.name %></option>
                          <%end%>
                      </select>
                      <div class="validation"></div>
                    </div>
                    <div class="form-group col-lg-6">
                      <label>Goal</label>
                      <input type="string" class="form-control" name="name" placeholder="Goal"  data-msg="Please enter a valid Goal" required="true" />
                      <div class="validation"></div>
                    </div>
                    <div class="form-group col-lg-6">
                      <label>Task</label>
                      <input type="string" class="form-control" name="task" placeholder="Task"  data-msg="Please enter a valid task" required="true" />
                      <div class="validation"></div>
                    </div>
                  </div>
                  <div class="form-group">
                    <label>Start Date</label>
                    <input type="date" class="form-control" name="start_date" id="subject" placeholder="Start Date" required="true" />
                    
                    <div class="validation"></div>
                  </div>
                  <div class="form-group">
                    <label>End Date</label>
                    <input type="date" class="form-control" name="end_date" id="subject" placeholder="End Date" required="true" />
                       
                    <div class="validation"></div>
                  </div>
                    
                  <div class="text-center"><button type="submit" title="Send Message">Create Goal</button></div>

                </form>
              </div>
            </div>

          </div>

        </div>
      </section><!-- #new-goal-form -->

      
      
    </div>
  </section><!-- #services -->
</main>

<a href="#" class="back-to-top"><i class="fa fa-chevron-up"></i></a>

<style type="text/css">
  .alert-success{
    color: green;
  }
  .alert-danger{
    color: red;
  }
</style>
